{
  "name": "app-starter-kit",
  "version": "1.0.0",
  "description": "Front End JS App with essential dev dependencies and LESS mixin library",
  "main": "webpack.common.js",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --config webpack.dev.js && nodemon app.js --exec babel-node",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "test_watch": "npm test -- --watch",
    "test_cov": "npm test -- --coverage .; true",
    "prepush": "npm test"
  },
  "author": "Rick Bergmann",
  "license": "ISC",
  "dependencies": {
    "axios": "latest",
    "express": "latest",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "latest",
    "react-router-dom": "latest",
    "react-scripts": "latest",
    "redux": "latest",
    "requirejs": "latest"
  },
  "devDependencies": {
    "babel-cli": "latest",
    "babel-core": "latest",
    "babel-jest": "latest",
    "babel-loader": "latest",
    "babel-plugin-transform-react-jsx": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "babel-preset-stage-2": "latest",
    "babel-register": "latest",
    "browser-sync": "latest",
    "browser-sync-webpack-plugin": "latest",
    "browsersync": "latest",
    "bs-html-injector": "latest",
    "clean-webpack-plugin": "latest",
    "css-loader": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-loader": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "^21.6.2",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-react": "latest",
    "extract-text-webpack-plugin": "latest",
    "html-webpack-plugin": "latest",
    "jest": "latest",
    "less": "latest",
    "less-loader": "latest",
    "nodemon": "latest",
    "optimize-css-assets-webpack-plugin": "latest",
    "path": "latest",
    "postcss-loader": "latest",
    "redux-devtools": "latest",
    "style-loader": "latest",
    "webpack": "latest",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-manifest-plugin": "latest",
    "webpack-merge": "latest"
  },
  "jest": {
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "notify": true,
    "collectCoverageFrom": [
      "./**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/lcov-report"
    ],
    "setupFiles": [
      "raf/polyfill"
    ]
  }
}
