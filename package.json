{
  "name": "app-starter-kit",
  "version": "1.0.0",
  "description": "Front End JS App with essential dev dependencies and LESS mixin library",
  "main": "webpack.common.js",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test_watch": "npm test -- --watch",
    "test_cov": "npm test -- --coverage .; true",
    "prepush": "npm test"
  },
  "author": "Rick Bergmann",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^8.0.0",
    "axios": "^0.17.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "v5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "requirejs": "^2.3.5",
    "webpack-cli": "^2.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.0.1",
    "bs-html-injector": "^3.0.3",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.10",
    "cypress": "^2.0.4",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.6.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.2",
    "less": "^2.3.1",
    "less-loader": "^4.0.5",
    "less-plugin-lists": "^1.1.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "path": "^0.12.7",
    "postcss-loader": "^v2.1.1",
    "redux-devtools": "^3.4.1",
    "style-loader": "latest",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.2"
  },
  "jest": {
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "notify": true,
    "collectCoverageFrom": [
      "./**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/lcov-report"
    ],
    "setupFiles": [
      "raf/polyfill"
    ]
  }
}
