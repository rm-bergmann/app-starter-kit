{
  "name": "app-starter-kit",
  "version": "1.1.1",
  "description": "Front End JS App with essential dev dependencies and LESS mixin library",
  "main": "webpack.common.js",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test_watch": "npm test -- --watch",
    "test_cov": "npm test -- --coverage .; true",
    "prepush": "npm test"
  },
  "author": "Rick Bergmann",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.4.7",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "base-64": "^0.1.0",
    "html-loader": "^0.5.5",
    "lodash": "^4.17.11",
    "markdown-loader": "^5.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.6.5",
    "react-html-parser": "^2.0.2",
    "react-markdown": "^4.0.6",
    "react-redux": "v6.0.0",
    "react-render-html": "^0.6.0",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "webpack-cli": "^3.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "mini-css-extract-plugin": "^0.5.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "less-plugin-lists": "^1.1.2",
    "less-style-library": "^1.1.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "redux-devtools": "^3.5.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.1",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.2"
  },
  "jest": {
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "notify": true,
    "collectCoverageFrom": [
      "./**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/lcov-report"
    ],
    "setupFiles": [
      "raf/polyfill"
    ]
  }
}
